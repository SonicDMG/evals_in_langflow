# ===========================
# LangSmith Configuration
# ===========================

# Enable or disable LangSmith tracing (true/false)
LANGSMITH_TRACING=true

# LangSmith API endpoint
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"

# Your LangChain API key (get from LangSmith dashboard)
LANGCHAIN_API_KEY="your-langchain-api-key"

# Name of your LangChain project
LANGCHAIN_PROJECT="your-project-name"

# ===========================
# OpenAI Configuration
# ===========================

# Your OpenAI API key (get from OpenAI dashboard)
OPENAI_API_KEY="your-openai-api-key"

# ===========================
# Langflow Configuration
# ===========================

# Your Langflow API key (uncomment and set if using Langflow)
LANGFLOW_API_KEY="your-langflow-api-key"